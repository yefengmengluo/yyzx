<script>

$(function(){
	//添加留言
    $("#addsave").click(function(){
    	$(this).attr("disabled","true");
    	$(this).attr("value","提交中");
    	
		var specId=$("#specId").val();
		var userId=$("#userId").val();
		var copName =$("#copName").val();
		var linkman =$("#linkman").val();
		var mobile =$("#mobile").val();
		var comment =$("#comment").val();
		 if($("#copName").val()=="请输入公司名称"||$("#copName").val()==""||$("#mobile").val()=="手机必填"|| $("#linkman").val()=="填写申请人的真实姓名"){
				alert("信息填写有误！请正确填写信息！");  
				$(".t1000-batch-btn1").removeAttr("disabled");
				$(this).attr("value","提交申请");
		  }else{
			
		$.ajax({
		    url:"/liuYanController/addLiuYan",
		    type: "POST",
			dataType : "json",
   		 	data:{"specId":specId,"userId":userId,"copName":copName,
		    	"linkman":linkman,"mobile":mobile,"comment":comment},
		    success:function(data){
		    
		    	if(data == 1){
		    	alert("留言成功");
		    	$(".t1000-batch-btn1").removeAttr("disabled");
		    	$(".t1000-batch-btn1").attr("value","提交申请");
		    	$(".reset").click();
		    	}else{
		    	alert("您已留言");
		    	$(".t1000-batch-btn1").removeAttr("disabled");
		    	$(".t1000-batch-btn1").attr("value","提交申请")
		    	$(".reset").click();
		    	}
		    	
		    }
		});
		  }
	})
})



